  "datasets": {
    "chebi": {
      "label": "ChEBI compound",
      "template": "https://raw.githubusercontent.com/togodx/togodx-config-tpp/develop/templates/chebi.hbs",
      "target": true,
      "examples": [
        "15996",
        "50114",
        "15377",
        "57540",
        "17154",
        "18367",
        "30616",
        "456216",
        "29108",
        "29105"
      ],
